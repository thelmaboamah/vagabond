<h2 class='text-center'>Community</h2>
<hr>

<div class='row'>
  <% @users.each do |user| %>
  <div class='col-md-3'>
    <ul class="users list-unstyled">
      <li>
        <%= image_tag("https://cdn.tutsplus.com/net/uploads/legacy/958_placeholders/placehold.gif") %>
      </li>
      <li class='text-center'>
        <%= link_to user.first_name, user %>
      </li>
    </ul>
  </div>
  <% end %>
  <%= will_paginate %>
</div><br><br>

